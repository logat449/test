;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ip2.grpx.mrt root.ip2.grpx.mrt (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	ip2.grpx.mrt.
10.10	IN	PTR	ip2.grpx.mrt.
11.10   IN      PTR     ip1.grpx.mrt.
12.10   IN      PTR     ip3.grpx.mrt.
